<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    android:clipChildren="false"
    android:clipToPadding="false"
    xmlns:folding-cell="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000"
    tools:context=".props.Update_Properties">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintBottom_toTopOf="@id/customBottomBar">


            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="20dp"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:background="@color/colorLightBlack"
                        android:paddingBottom="1dp"
                        android:clickable="true"
                        android:id="@+id/menu_side_bar"
                        android:layout_width="110dp"
                        android:layout_height="match_parent">

                        <android.support.v4.widget.NestedScrollView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <LinearLayout
                                android:layout_centerInParent="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginLeft="4dp"
                                android:layout_marginRight="4dp"
                                android:layout_marginTop="4dp"
                                android:layout_marginBottom="32dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:layout_centerInParent="true"
                                    android:padding="8dp"
                                    android:layout_gravity="center"
                                    android:src="@drawable/profile_image"
                                    android:id="@+id/profile_image"
                                    android:layout_width="70dp"
                                    android:layout_height="70dp" />

                                <TextView
                                    android:textColor="@color/white"
                                    android:layout_gravity="center"
                                    android:textSize="12sp"
                                    android:layout_marginBottom="55dp"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="username"
                                    android:textStyle="bold"/>

                                <android.support.v7.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="4dp"
                                    android:background="@drawable/circle"
                                    android:clickable="true"
                                    android:focusable="true"
                                    android:foreground="?attr/selectableItemBackground"
                                    app:cardCornerRadius="10dp">

                                    <ImageView
                                        android:id="@+id/img_drugs"
                                        android:layout_width="90dp"
                                        android:layout_height="90dp"
                                        android:layout_centerInParent="true"
                                        android:padding="16dp"
                                        android:src="@drawable/setting_user"
                                        />

                                </android.support.v7.widget.CardView>

                                <android.support.v7.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="4dp"
                                    android:clickable="true"
                                    android:focusable="true"
                                    android:foreground="?attr/selectableItemBackground"
                                    app:cardCornerRadius="10dp">

                                    <ImageView
                                        android:id="@+id/img_profile"
                                        android:layout_width="90dp"
                                        android:layout_height="90dp"
                                        android:layout_centerInParent="true"
                                        android:padding="16dp"
                                        android:src="@drawable/name_plate"
                                        />

                                </android.support.v7.widget.CardView>

                                <android.support.v7.widget.CardView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_margin="4dp"
                                    android:clickable="true"
                                    android:focusable="true"
                                    android:foreground="?attr/selectableItemBackground"
                                    app:cardCornerRadius="10dp">

                                    <ImageView
                                        android:id="@+id/img_drugHistory"
                                        android:layout_width="90dp"
                                        android:layout_height="90dp"
                                        android:layout_centerInParent="true"
                                        android:padding="16dp"
                                        android:src="@drawable/best_seller"
                                        />

                                </android.support.v7.widget.CardView>

                                <TextView
                                    android:id="@+id/createPropery"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="40dp"
                                    android:layout_marginBottom="35dp"
                                    android:rotation="-90"
                                    android:text="Remove Property"
                                    android:textColor="@color/white"
                                    android:textSize="15sp"
                                    android:textStyle="bold"
                                    android:tint="@color/icon_tint_color" />

                                <TextView
                                    android:id="@+id/updateProperties"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="25dp"
                                    android:layout_marginBottom="35dp"
                                    android:rotation="-90"
                                    android:text="Create Property"
                                    android:textColor="@color/white"
                                    android:textSize="15sp"
                                    android:textStyle="bold"
                                    android:tint="@color/icon_tint_color" />

                                <TextView
                                    android:id="@+id/sideMenu"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="25dp"
                                    android:layout_marginBottom="60dp"
                                    android:rotation="-90"
                                    android:text="Update Property"
                                    android:textColor="@color/white"
                                    android:textSize="15sp"
                                    android:textStyle="bold"
                                    android:tint="@color/icon_tint_color" />


                            </LinearLayout>

                        </android.support.v4.widget.NestedScrollView>

                    </RelativeLayout>

                    <android.support.v4.widget.NestedScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <RelativeLayout
                            android:layout_marginBottom="30dp"
                            android:id="@+id/main_board"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <RelativeLayout
                                android:background="@color/colorLightBlack"
                                android:id="@+id/relative_layout_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <ImageView
                                    android:id="@+id/back_retail_plan"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:src="@drawable/arrow_left"
                                    android:layout_margin="16dp"
                                    android:layout_centerInParent="true"
                                    android:layout_alignParentStart="true"
                                    />

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Update Details of Property"
                                    android:textColor="#ffffff"
                                    android:textSize="15sp"
                                    android:layout_centerInParent="true"
                                    android:textStyle="bold"
                                    />

                                <ImageView
                                    android:id="@+id/menu_retail_plan"
                                    android:layout_alignParentEnd="true"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:src="@drawable/menu_hamburger"
                                    android:layout_centerInParent="true"
                                    android:layout_margin="16dp"
                                    />

                            </RelativeLayout>

                            <LinearLayout
                                android:layout_below="@+id/relative_layout_1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="center_horizontal"
                                    android:orientation="vertical">

                                    <ImageView
                                        android:id="@+id/profile_background"
                                        android:layout_width="match_parent"
                                        android:layout_height="200dp"
                                        android:foreground="@color/colorTransparentBlack"
                                        android:scaleType="centerCrop"
                                        android:src="@drawable/profile_background" />

                                    <com.mikhaellopez.circularimageview.CircularImageView
                                        android:id="@+id/property_image"
                                        android:layout_width="200dp"
                                        android:layout_height="200dp"
                                        android:layout_marginTop="-155dp"
                                        android:elevation="10dp"
                                        android:src="@drawable/house"
                                        app:civ_border="true"
                                        app:civ_border_color="@color/colorWhite"
                                        app:civ_border_width="4dp"
                                        app:civ_shadow="true"
                                        app:civ_shadow_radius="0" />

                                    <com.mikhaellopez.circularimageview.CircularImageView
                                        android:id="@+id/profile_add_image"
                                        android:layout_width="50dp"
                                        android:layout_height="50dp"
                                        android:layout_marginLeft="60dp"
                                        android:layout_marginTop="-45dp"
                                        android:elevation="10dp"
                                        android:src="@drawable/add_profile_picture"
                                        app:civ_border="true"
                                        app:civ_border_color="@color/colorWhite"
                                        app:civ_border_width="4dp"
                                        app:civ_shadow="true"
                                        app:civ_shadow_radius="0" />

                                    <LinearLayout
                                        android:orientation="horizontal"
                                        android:gravity="center"
                                        android:layout_marginBottom="30dp"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:id="@+id/number_of_added_property"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginTop="10dp"
                                            android:elevation="10dp"
                                            android:text="Added Property Images"
                                            android:textSize="15sp"
                                            android:textAppearance="@style/Base.TextAppearance.AppCompat.Title"
                                            android:textColor="@color/cardview_light_background" />

                                    </LinearLayout>

                                    <android.support.v7.widget.CardView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="10dp"
                                        android:layout_marginTop="15dp"
                                        android:layout_marginRight="10dp"
                                        android:layout_marginBottom="10dp"
                                        android:visibility="visible"
                                        app:cardCornerRadius="2dp"
                                        app:cardElevation="2dp">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:layout_marginTop="50dp"
                                            android:orientation="horizontal">

                                            <ProgressBar
                                                android:visibility="gone"
                                                android:layout_width="50dp"
                                                android:layout_height="50dp"
                                                android:layout_marginStart="100dp"
                                                android:id="@+id/progressBar_profile" />

                                            <TextView
                                                android:visibility="gone"
                                                android:id="@+id/please_wait_profile"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginStart="40dp"
                                                android:text="Please Wait"
                                                android:textSize="15sp"
                                                android:layout_marginTop="12dp"
                                                />

                                        </LinearLayout>

                                        <!--whole body card layout -->

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:gravity="center_horizontal"
                                            android:orientation="vertical">

                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_margin="10dp"
                                                android:text="Update details of your new property \n\ that you want to market"
                                                android:textColor="@color/colorDark"
                                                android:textSize="14sp"
                                                android:textStyle="bold" />


                                            <LinearLayout
                                                android:paddingStart="20dp"
                                                android:paddingEnd="20dp"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:orientation="vertical"
                                                android:layout_marginTop="30dp"
                                                android:paddingBottom="50dp">

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Username"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/user_name_property"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Property Name"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/name_property_create"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Property Price"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/price_property_create"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Property State"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <Spinner
                                                    android:entries="@array/property_state"
                                                    android:textSize="15sp"
                                                    android:id="@+id/property_state"
                                                    android:layout_width="190dp"
                                                    android:layout_height="50dp"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:background="@drawable/edittext_field_background_combined"
                                                    android:paddingStart="10dp" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Bed Count"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <Spinner
                                                    android:entries="@array/bed_count"
                                                    android:textSize="15sp"
                                                    android:id="@+id/property_bed_count"
                                                    android:layout_width="190dp"
                                                    android:layout_height="50dp"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:background="@drawable/edittext_field_background_combined"
                                                    android:paddingStart="10dp" />


                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Parking Count"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <Spinner
                                                    android:entries="@array/parking_count"
                                                    android:textSize="15sp"
                                                    android:id="@+id/parking_count"
                                                    android:layout_width="190dp"
                                                    android:layout_height="50dp"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:background="@drawable/edittext_field_background_combined"
                                                    android:paddingStart="10dp" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Bath Count"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <Spinner
                                                    android:entries="@array/bath_count"
                                                    android:textSize="15sp"
                                                    android:id="@+id/bath_count_property"
                                                    android:layout_width="190dp"
                                                    android:layout_height="50dp"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:background="@drawable/edittext_field_background_combined"
                                                    android:paddingStart="10dp" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Property Make"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/property_make"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Property Description"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/property_description"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Property_owner"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/property_owner"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Office Address"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/office_address_property"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Email Address"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/email_property"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="textEmailAddress" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Mobile Number"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/mobile_number_property"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="text|phone" />

                                                <TextView
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:layout_margin="10dp"
                                                    android:text="Date created"
                                                    android:textColor="@color/colorNormalText"
                                                    android:textSize="14sp"
                                                    />

                                                <EditText
                                                    android:id="@+id/date_of_birth_editProfile"
                                                    android:layout_width="match_parent"
                                                    android:layout_height="wrap_content"
                                                    android:layout_marginLeft="10dp"
                                                    android:layout_marginRight="10dp"
                                                    android:inputType="datetime" />

                                            </LinearLayout>











                                            <!--button layout -->

                                            <LinearLayout
                                                android:layout_gravity="center"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:layout_marginLeft="15dp"
                                                android:layout_marginTop="25dp"
                                                android:layout_marginRight="15dp"
                                                android:layout_marginBottom="30dp"
                                                android:orientation="horizontal"
                                                android:weightSum="2">

                                                <Button
                                                    android:layout_marginHorizontal="40dp"
                                                    android:id="@+id/create_property"
                                                    android:layout_width="200dp"
                                                    android:layout_height="52dp"
                                                    android:layout_weight="1"
                                                    android:background="@drawable/request_button"
                                                    android:text="Create Property"
                                                    android:layout_marginRight="10dp"
                                                    android:textColor="@color/colorWhite" />

                                            </LinearLayout>


                                        </LinearLayout>

                                    </android.support.v7.widget.CardView>

                                    <View
                                        android:layout_width="0dp"
                                        android:layout_height="15dp" />

                                    <LinearLayout
                                        android:layout_marginBottom="30dp"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:gravity="center_horizontal"
                                        android:orientation="horizontal"
                                        android:padding="15dp">

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:minWidth="80dp"
                                            android:orientation="vertical">

                                            <android.support.design.widget.FloatingActionButton
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:clickable="true"
                                                app:backgroundTint="@android:color/white"
                                                app:elevation="1dp"
                                                app:fabSize="normal"
                                                app:rippleColor="@color/colorGrey"
                                                app:srcCompat="@drawable/icon_facebook" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:minWidth="80dp"
                                            android:orientation="vertical">

                                            <android.support.design.widget.FloatingActionButton
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:clickable="true"
                                                app:backgroundTint="@android:color/white"
                                                app:elevation="1dp"
                                                app:fabSize="normal"
                                                app:rippleColor="@color/colorGrey"
                                                app:srcCompat="@drawable/icon_whatsapp" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:minWidth="80dp"
                                            android:orientation="vertical">

                                            <android.support.design.widget.FloatingActionButton
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:clickable="true"
                                                app:backgroundTint="@android:color/white"
                                                app:elevation="1dp"
                                                app:fabSize="normal"
                                                app:rippleColor="@color/colorGrey"
                                                app:srcCompat="@drawable/icon_youtube" />
                                        </LinearLayout>

                                        <LinearLayout
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="center"
                                            android:minWidth="80dp"
                                            android:orientation="vertical">

                                            <android.support.design.widget.FloatingActionButton
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:clickable="true"
                                                app:backgroundTint="@android:color/white"
                                                app:elevation="1dp"
                                                app:fabSize="normal"
                                                app:rippleColor="@color/colorGrey"
                                                app:srcCompat="@drawable/icon_linkedn" />
                                        </LinearLayout>
                                    </LinearLayout>





                                </LinearLayout>




                            </LinearLayout>

                        </RelativeLayout>


                    </android.support.v4.widget.NestedScrollView>

                </LinearLayout>


            </LinearLayout>



        </RelativeLayout>





